{
  "manifest_version": 2,
  "name": "incognito-meet",
  "description": "Incognito meet",
  "version": "1.0.0",
  "author": "Himanshu chhabra",
  "permissions": ["declarativeContent", "storage", "<all_urls>", "activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "css": ["content_script/myStyles.css"],
      "js": ["content_script/contentScript.js"]
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/eye-mask16.png",
      "32": "assets/eye-mask32.png"
    }
  },
  "icons": {
    "16": "assets/eye-mask16.png",
    "32": "assets/eye-mask32.png"
  }
}
